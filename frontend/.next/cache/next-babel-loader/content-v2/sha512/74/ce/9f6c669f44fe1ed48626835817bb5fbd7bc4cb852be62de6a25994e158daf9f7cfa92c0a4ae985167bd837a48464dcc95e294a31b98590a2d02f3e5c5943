{\n    method: 'PUT',\n    headers: {\n      Accept: 'application/json',\n      'Content-Type': 'application/json'\n    },\n    body: JSON.stringify(resetInfo)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};\nexport var loginWithGoogle = function loginWithGoogle(user) {\n  return fetch(\"\".concat(API, \"/google-login\"), {\n    method: 'POST',\n    headers: {\n      Accept: 'application/json',\n      'Content-Type': 'application/json'\n    },\n    body: JSON.stringify(user)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (err) {\n    return console.log(err);\n  });\n};","map":{"version":3,"sources":["/home/jay/Desktop/node/SPE_MAJOR/frontend/actions/auth.js"],"names":["fetch","cookie","API","Router","React","handleResponse","response","status","signout","push","pathname","query","message","preSignup","user","method","headers","Accept","body","JSON","stringify","then","json","err","console","log","signup","signin","next","removeCookie","removeLocalStorage","setCookie","key","value","set","expires","remove","getCookie","get","setLocalStorage","localStorage","setItem","removeItem","authenticate","data","token","isAuth","cookieChecked","getItem","parse","updateUser","auth","forgotPassword","email","resetPassword","resetInfo","loginWithGoogle"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,kBAAlB;AACA,OAAOC,MAAP,MAAmB,WAAnB;AACA,SAASC,GAAT,QAAoB,WAApB;AACA,OAAOC,MAAP,MAAmB,aAAnB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,OAAO,IAAMC,cAAc,GAAG,SAAjBA,cAAiB,CAAAC,QAAQ,EAAI;AACtC,MAAIA,QAAQ,CAACC,MAAT,KAAoB,GAAxB,EAA6B;AACzBC,IAAAA,OAAO,CAAC,YAAM;AACVL,MAAAA,MAAM,CAACM,IAAP,CAAY;AACRC,QAAAA,QAAQ,EAAE,SADF;AAERC,QAAAA,KAAK,EAAE;AACHC,UAAAA,OAAO,EAAE;AADN;AAFC,OAAZ;AAMH,KAPM,CAAP;AAQH;AACJ,CAXM;AAYP,OAAO,IAAMC,SAAS,GAAG,SAAZA,SAAY,CAAAC,IAAI,EAAI;AAC7B,SAAOd,KAAK,WAAIE,GAAJ,kBAAsB;AAC9Ba,IAAAA,MAAM,EAAE,MADsB;AAE9BC,IAAAA,OAAO,EAAE;AACLC,MAAAA,MAAM,EAAE,kBADH;AAEL,sBAAgB